{
    "Description": "AWS CloudFormation Template CloudFront_S3",
    "Outputs": {
        "DistributionId": {
            "Value": {
                "Ref": "myDistribution"
            }
        },
        "DistributionName": {
            "Value": {
                "Fn::Join": [
                    "",
                    [
                        "http://",
                        {
                            "Fn::GetAtt": [
                                "myDistribution",
                                "DomainName"
                            ]
                        }
                    ]
                ]
            }
        }
    },
    "Parameters": {
        "S3DNSNAme": {
            "Description": "The DNS name of an existing S3 bucket to use as the Cloudfront distribution origin",
            "Type": "String"
        },
        "S3Origin": {
            "Type": "String"
        },
        "aliasCNAME": {
            "Description": "Alternate Domain Names (CNAMEs)",
            "Type": "String"
        },
        "behaviorACCOUNT": {
            "Type": "String"
        },
        "behaviorALERTS": {
            "Type": "String"
        },
        "behaviorBENEFITS": {
            "Type": "String"
        },
        "behaviorENROLLMENT": {
            "Type": "String"
        },
        "behaviorFULFILLMENT": {
            "Type": "String"
        },
        "behaviorOFFERS": {
            "Type": "String"
        },
        "behaviorPROFILE": {
            "Type": "String"
        },
        "behaviorREGISTRATION": {
            "Type": "String"
        },
        "behaviorREPORT": {
            "Type": "String"
        },
        "dnsACCOUNT": {
            "Type": "String"
        },
        "dnsALERTS": {
            "Type": "String"
        },
        "dnsBENEFITS": {
            "Type": "String"
        },
        "dnsENROLLMENT": {
            "Type": "String"
        },
        "dnsFULFILLMENT": {
            "Type": "String"
        },
        "dnsOFFERS": {
            "Type": "String"
        },
        "dnsPROFILE": {
            "Type": "String"
        },
        "dnsREGISTRATION": {
            "Type": "String"
        },
        "dnsREPORT": {
            "Type": "String"
        },
        "originACCOUNT": {
            "Type": "String"
        },
        "originALERTS": {
            "Type": "String"
        },
        "originBENEFITS": {
            "Type": "String"
        },
        "originENROLLMENT": {
            "Type": "String"
        },
        "originFULFILLMENT": {
            "Type": "String"
        },
        "originOFFERS": {
            "Type": "String"
        },
        "originPROFILE": {
            "Type": "String"
        },
        "originREGISTRATION": {
            "Type": "String"
        },
        "originREPORT": {
            "Type": "String"
        }
    },
    "Resources": {
        "myDistribution": {
            "Properties": {
                "DistributionConfig": {
                    "Aliases": [
                        {
                            "Ref": "aliasCNAME"
                        }
                    ],
                    "CacheBehaviors": [
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": {
                                "Ref": "behaviorACCOUNT"
                            },
                            "TargetOriginId": {
                                "Ref": "originACCOUNT"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": {
                                "Ref": "behaviorREPORT"
                            },
                            "TargetOriginId": {
                                "Ref": "originREPORT"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": {
                                "Ref": "behaviorALERTS"
                            },
                            "TargetOriginId": {
                                "Ref": "originALERTS"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": {
                                "Ref": "behaviorBENEFITS"
                            },
                            "TargetOriginId": {
                                "Ref": "originBENEFITS"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": {
                                "Ref": "behaviorENROLLMENT"
                            },
                            "TargetOriginId": {
                                "Ref": "originENROLLMENT"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": {
                                "Ref": "behaviorFULFILLMENT"
                            },
                            "TargetOriginId": {
                                "Ref": "originFULFILLMENT"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": {
                                "Ref": "behaviorOFFERS"
                            },
                            "TargetOriginId": {
                                "Ref": "originOFFERS"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": {
                                "Ref": "behaviorPROFILE"
                            },
                            "TargetOriginId": {
                                "Ref": "originPROFILE"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": {
                                "Ref": "behaviorREGISTRATION"
                            },
                            "TargetOriginId": {
                                "Ref": "originREGISTRATION"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        }
                    ],
                    "DefaultCacheBehavior": {
                        "ForwardedValues": {
                            "QueryString": "true"
                        },
                        "TargetOriginId": {
                            "Ref": "S3Origin"
                        },
                        "ViewerProtocolPolicy": "allow-all"
                    },
                    "Enabled": "true",
                    "Origins": [
                        {
                            "DomainName": {
                                "Ref": "S3DNSNAme"
                            },
                            "Id": {
                                "Ref": "S3Origin"
                            },
                            "S3OriginConfig": {
                                "OriginAccessIdentity": ""
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsACCOUNT"
                            },
                            "Id": {
                                "Ref": "originACCOUNT"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsREPORT"
                            },
                            "Id": {
                                "Ref": "originREPORT"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsALERTS"
                            },
                            "Id": {
                                "Ref": "originALERTS"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsBENEFITS"
                            },
                            "Id": {
                                "Ref": "originBENEFITS"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsENROLLMENT"
                            },
                            "Id": {
                                "Ref": "originENROLLMENT"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsFULFILLMENT"
                            },
                            "Id": {
                                "Ref": "originFULFILLMENT"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsOFFERS"
                            },
                            "Id": {
                                "Ref": "originOFFERS"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsPROFILE"
                            },
                            "Id": {
                                "Ref": "originPROFILE"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsREGISTRATION"
                            },
                            "Id": {
                                "Ref": "originREGISTRATION"
                            }
                        }
                    ]
                }
            },
            "Type": "AWS::CloudFront::Distribution"
        }
    }
}
