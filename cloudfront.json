{
    "Description": "AWS CloudFormation Template CloudFront_S3",
    "Outputs": {
        "DistributionId": {
            "Value": {
                "Ref": "myDistribution"
            }
        },
        "DistributionName": {
            "Value": {
                "Fn::Join": [
                    "",
                    [
                        "http://",
                        {
                            "Fn::GetAtt": [
                                "myDistribution",
                                "DomainName"
                            ]
                        }
                    ]
                ]
            }
        }
    },
    "Parameters": {
        "S3DNSNAme": {
            "Description": "The DNS name of an existing S3 bucket to use as the Cloudfront distribution origin",
            "Type": "String"
        },
        "S3Origin": {
            "Type": "String"
        },
        "aliasCNAME": {
            "Description": "Alternate Domain Names (CNAMEs)",
            "Type": "String"
        },
        "dnsEIGHT": {
            "Type": "String"
        },
        "dnsFIVE": {
            "Type": "String"
        },
        "dnsFOUR": {
            "Type": "String"
        },
        "dnsNINE": {
            "Type": "String"
        },
        "dnsSEVEN": {
            "Type": "String"
        },
        "dnsSIX": {
            "Type": "String"
        },
        "dnsTEN": {
            "Type": "String"
        },
        "dnsTHREE": {
            "Type": "String"
        },
        "dnsTWO": {
            "Type": "String"
        },
        "originEIGHT": {
            "Type": "String"
        },
        "originFIVE": {
            "Type": "String"
        },
        "originFOUR": {
            "Type": "String"
        },
        "originNINE": {
            "Type": "String"
        },
        "originSEVEN": {
            "Type": "String"
        },
        "originSIX": {
            "Type": "String"
        },
        "originTEN": {
            "Type": "String"
        },
        "originTHREE": {
            "Type": "String"
        },
        "originTWO": {
            "Type": "String"
        }
    },
    "Resources": {
        "myDistribution": {
            "Properties": {
                "DistributionConfig": {
                    "Aliases": [
                        {
                            "Ref": "aliasCNAME"
                        }
                    ],
                    "CacheBehaviors": [
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": "/api/account",
                            "TargetOriginId": {
                                "Ref": "originTWO"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": "/api/report",
                            "TargetOriginId": {
                                "Ref": "originTHREE"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": "/api/alerts",
                            "TargetOriginId": {
                                "Ref": "originFOUR"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": "/api/benefits",
                            "TargetOriginId": {
                                "Ref": "originFIVE"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": "/api/enrollment",
                            "TargetOriginId": {
                                "Ref": "originSIX"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": "/api/fulfillment",
                            "TargetOriginId": {
                                "Ref": "originSEVEN"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": "/api/offers",
                            "TargetOriginId": {
                                "Ref": "originEIGHT"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": "/api/profile",
                            "TargetOriginId": {
                                "Ref": "originNINE"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        },
                        {
                            "ForwardedValues": {
                                "QueryString": "true"
                            },
                            "PathPattern": "/api/registration",
                            "TargetOriginId": {
                                "Ref": "originTEN"
                            },
                            "ViewerProtocolPolicy": "allow-all"
                        }
                    ],
                    "DefaultCacheBehavior": {
                        "ForwardedValues": {
                            "QueryString": "true"
                        },
                        "TargetOriginId": {
                            "Ref": "S3Origin"
                        },
                        "ViewerProtocolPolicy": "allow-all"
                    },
                    "Enabled": "true",
                    "Origins": [
                        {
                            "DomainName": {
                                "Ref": "S3DNSNAme"
                            },
                            "Id": {
                                "Ref": "S3Origin"
                            },
                            "S3OriginConfig": {
                                "OriginAccessIdentity": ""
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsTWO"
                            },
                            "Id": {
                                "Ref": "originTWO"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsTHREE"
                            },
                            "Id": {
                                "Ref": "originTHREE"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsFOUR"
                            },
                            "Id": {
                                "Ref": "originFOUR"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsFIVE"
                            },
                            "Id": {
                                "Ref": "originFIVE"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsSIX"
                            },
                            "Id": {
                                "Ref": "originSIX"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsSEVEN"
                            },
                            "Id": {
                                "Ref": "originSEVEN"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsEIGHT"
                            },
                            "Id": {
                                "Ref": "originEIGHT"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsNINE"
                            },
                            "Id": {
                                "Ref": "originNINE"
                            }
                        },
                        {
                            "CustomOriginConfig": {
                                "HTTPPort": "80",
                                "HTTPSPort": "443",
                                "OriginProtocolPolicy": "http-only"
                            },
                            "DomainName": {
                                "Ref": "dnsTEN"
                            },
                            "Id": {
                                "Ref": "originTEN"
                            }
                        }
                    ]
                }
            },
            "Type": "AWS::CloudFront::Distribution"
        }
    }
}
